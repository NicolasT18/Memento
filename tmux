Launch Tmux :
	tmux a

Show Tmux session :
	tmux ls


SHORTCUTS : Ctrl+b   #For launch tmux shorctus

	Pane :

	c	#Create new pane (* show the current pane)
	d	#Delete the current pane
	,	#Rename the current pane
	n	#Navigate to the next pane
	p	#Navigate to the previous pane
	<num>	#Navigate to the number pane

	Windows :

	%	#Cut the window vertically
	"	#Cut the window horizontally
	Alt+<-	#Change the window dimensions (Alt+arrow)
	o	#Navigate to the next window
	q<num>	#Navigate to the number window
	z	#On/off fullscreen for the current window
